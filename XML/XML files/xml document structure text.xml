<?xml version="1.0" encoding="UTF-8"?>
<myCar-details>
    <_xml-data>version 1.0</_xml-data>          <!-- tag cannot start with "xml" but in this case underscore solves this issue -->
    <_name>Audi</_name>                         <!-- tagname starts with an underscore -->
    <_Name>A4</_Name>                           <!-- tagnames are casse sensitive --> 
    <year-of.issue1>2010</year-of.issue1>       <!-- allowed characters used -->
    <issue-month data=""/>                      <!-- self closing tag, because boolean attributes are not allowed an empty attribute value is declared here -->
    <engine bigger-than="&gt;2.0" />            <!-- entity reference used as attribute value -->
    <property-history>
        <_1>geramny</_1>
        <_2>me :)</_2>
    </property-history>
</myCar-details>